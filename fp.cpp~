#include<iostream>
using namespace std;

int main () {
int x[5]={1,2,3,4,5};
for(int i=0;i<5;i++)
cout<<x[i]<<" ";
char name[50]="my_name";
cout<<"enter first name:";
cin>>name;
for(int a=0;a<50;a++);
{
if(name[a]=='\0')break;
{cout<<name[a]<<endl;
}
}
//print using loop

//print name using loop "Don't use cout<<name"
return 0;
}
